{
	"name": "gifsicle",
	"version": "7.0.1",
	"description": "gifsicle wrapper that makes it seamlessly available as a local dependency",
	"license": "MIT",
	"repository": "imagemin/gifsicle-bin",
	"type": "module",
	"exports": "./index.js",
	"funding": {
		"url": "https://github.com/imagemin/gifsicle-bin?sponsor=1"
	},
	"bin": {
		"gifsicle": "cli.js"
	},
	"engines": {
		"node": "^14.13.1 || >=16.0.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
